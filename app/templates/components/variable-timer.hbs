<h1>Duration</h1>

<section>
    {{log 'timerText ' timerText}}
    <div>Timer: {{timerText}}</div>
    <div>Duration Level: {{durationText}}</div>
    <div>Lower Limit: {{lowerLimit}} </div>
    <div>Upper Limit: {{upperLimit}} </div>
    <div>Iteration: {{iteration}} </div>
    <button onClick={{action 'incrementDuration'}}>Pass</button>
    <button onClick={{action 'startTimer' duration}}>Start Timer</button>
</section>